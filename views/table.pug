doctype html  
html(lang='en')  
 head
    meta(charset="UTF-8")
    meta(name="viewport" content="width=device-width, initial-scale=1")
    title #{taskId}
    link(rel="stylesheet", href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css", integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7", crossorigin="anonymous")
 body
    div(class="container")
      h1(style={"text-align": "center"})
        a(href=taskLink) Link
      table(class="table table-bordered table-hover")
        thead
          tr
            th(scope="col" class="info col-md-2" style={"text-align": "center"}) Cuboid ID 
            th(scope="col" class="info col-md-2" style={"text-align": "center"}) Error Type
            th(scope="col" class="info col-md-2" style={"text-align": "center"}) Label
            //- th Nuro ID
            th(scope="col" class="info col-md-2" style={"text-align": "center"}) Dimensions [x, y, z]
            th(scope="col" class="info col-md-2" style={"text-align": "center"}) Position [x, y, z]
            th(scope="col" class="info col-md-2") Frames
          tbody   
            each val in report
              tr
                  td #{val.cuboidId}
                  td #{val.error_type}
                  td #{val.label}
                  //- td #{val.nuro_id}
                  td #{val.dimensions}
                  td #{val.position}
                  td #{val.frames}